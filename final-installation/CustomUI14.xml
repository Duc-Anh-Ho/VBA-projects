<!-- Check README.md for more information -->
<customUI
  xmlns="http://schemas.microsoft.com/office/2009/07/customui"
  onLoad="customUIOnLoad">
  <ribbon startFromScratch="false">
    <tabs>
      <tab id="danh-tools" getLabel="getLabel" getVisible="getVisible">
        <group id="sheets-controller"
          getLabel="getLabel"
          image="SheetController">
          <button id="add-sheets"
            getSize="getSize"
            image="SheetsAddImg"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="sheetController" />
          <splitButton id="list-sheets-split"
            getSize="getSize">
            <button id="list-sheets"
              image="SheetsListImg"
              getEnabled="getEnabled"
              getShowImage="getShowImage"
              getKeytip="getKeytip"
              getLabel="getLabel"
              getShowLabel="getShowLabel"
              getScreentip="getScreentip"
              getSupertip="getSupertip"
              onAction="sheetControllerEvent" />
            <menu id="list-sheets-menu"
              itemSize="large">
              <button id="rename-sheets"
                getImage="getImage"
                getEnabled="getEnabled"
                getShowImage="getShowImage"
                getKeytip="getKeytip"
                getLabel="getLabel"
                getShowLabel="getShowLabel"
                getScreentip="getScreentip"
                getSupertip="getSupertip"
                getVisible="getVisible"
                onAction="sheetControllerEvent" />
            </menu>
          </splitButton>
          <button id="delete-sheets"
            getSize="getSize"
            image="SheetsDeleteImg"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="sheetController" />
          <separator id="sheet-separator-mid" />
          <button id="show-sheets"
            getSize="getSize"
            image="SheetsShowImg"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="sheetController" />
          <splitButton id="hide-sheets-split"
            getSize="getSize">
            <button id="hide-sheets"
              image="SheetsHideImg"
              getEnabled="getEnabled"
              getShowImage="getShowImage"
              getKeytip="getKeytip"
              getLabel="getLabel"
              getShowLabel="getShowLabel"
              getScreentip="getScreentip"
              getSupertip="getSupertip"
              onAction="sheetController" />
            <menu id="hide-sheets-menu"
              itemSize="large">
              <button id="very-hide-sheets"
                image="SheetsVeryHideImg"
                getEnabled="getEnabled"
                getShowImage="getShowImage"
                getKeytip="getKeytip"
                getLabel="getLabel"
                getShowLabel="getShowLabel"
                getScreentip="getScreentip"
                getSupertip="getSupertip"
                getVisible="getVisible"
                onAction="sheetController" />
            </menu>
          </splitButton>
        </group>
        <group id="charts-controller"
          getLabel="getLabel"
          imageMso="ControlChart">
          <button id="hide-error-labels"
            getSize="getSize"
            image="ChartsHideErrorLabelImg"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="chartController" />
          <button id="show-labels"
            getSize="getSize"
            image="ChartsShowLabelImg"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="chartController" />
        </group>
        <group id="pivot-controller"
          getLabel="getLabel"
          imageMso="PivotTableReport">
          <toggleButton id="refresh-pivot"
            getSize="getSize"
            getImage="getImage"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="pivotControllerEvent" />
        </group>
        <group id="vba-files-controller"
          getLabel="getLabel"
          imageMso="SaveAll">
          <button id="import-vba-files"
            getSize="getSize"
            image="ImportImg"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="VBAFilesController" />
          <button id="import-all-vba-files"
            getSize="getSize"
            image="ImportAllImg"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="VBAFilesController" />
          <separator id="vba-file-separator-mid" />
          <button id="export-all-vba-files"
            getSize="getSize"
            image="ExportAllImg"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="VBAFilesController" />
        </group>
        <group id="ranges-controller"
          getLabel="getLabel"
          image="Author">
          <dropDown id="hide-page-break"
            sizeString="AAAAA"
            imageMso="PageBreaksShow"
            getShowImage="getShowImage"
            getLabel="getLabel"
            getEnabled="getEnabled"
            getVisible="getVisible"
            getSelectedItemIndex="getSelectedItemIndex"
            onAction="hidePageBreakChange">
            <item
              id="hide-page-break-item"
              label="Hide" />
            <item
              id="show-page-break-item"
              label="Show" />
            <item
              id="hide-all-page-break-item"
              label="Hide All" />
            <item
              id="show-all-page-break-item"
              label="Show All" />
          </dropDown>
          <button id="multiple-replace"
            getSize="getSize"
            imageMso="ReplaceDialog"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="rangeController" />
          <separator id="range-separator-third" />
          <button id="bold-first-line"
            getSize="getSize"
            image="BoldFIrstLineImg"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="rangeController" />
          <separator id="range-separator-second" />
          <button id="invert-color"
            getSize="getSize"
            image="InvertColorImg"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="rangeController" />
          <separator id="range-separator-first" />
          <splitButton id="highlight-split"
            getSize="getSize">
            <toggleButton id="highlight-range"
              image="HighlightYellowImg"
              getEnabled="getEnabled"
              getShowImage="getShowImage"
              getKeytip="getKeytip"
              getLabel="getLabel"
              getShowLabel="getShowLabel"
              getScreentip="getScreentip"
              getSupertip="getSupertip"
              onAction="rangeControllerEvent" />
            <menu id="highlight-menu"
              itemSize="normal">
              <toggleButton id="highlight-bold"
                imageMso="Bold"
                label="Bold"
                getPressed="getPressed"
                onAction="rangeController" />
              <menu id="highlight-size-menu"
                label="Increase Size"
                imageMso="FontSizeIncrease">
                <toggleButton id="highlight-size-none"
                  label="None"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-size-one"
                  label="+1"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-size-two"
                  label="+2"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-size-three"
                  label="+3"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-size-four"
                  label="+4"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-size-five"
                  label="+5"
                  getPressed="getPressed"
                  onAction="rangeController" />
              </menu>
              <menu id="highlight-transparent-menu"
                label="Transparent Rate "
                imageMso="PictureSetTransparentColor">
                <toggleButton id="highlight-transparent-none"
                  label="0%"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-transparent-quarter"
                  label="25%"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-transparent-half"
                  label="50%"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-transparent-three-quarter"
                  label="75%"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-transparent-full"
                  label="100%"
                  getPressed="getPressed"
                  onAction="rangeController" />
              </menu>
              <menu id="highlight-color-menu"
                label="Color"
                imageMso="RecolorColorPicker">
                <toggleButton id="highlight-color-yellow"
                  image="HighlightYellowImg"
                  label="Yelow"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-color-cyan"
                  image="HighlightCyanImg"
                  label="Cyan"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-color-magenta"
                  image="HighlightMagentaImg"
                  label="Magenta"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-color-green"
                  image="HighlightGreenImg"
                  label="Green"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-color-red"
                  image="HighlightRedImg"
                  label="Red"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-color-blue"
                  image="HighlightBlueImg"
                  label="Blue"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-color-black"
                  image="HighlightBlackImg"
                  label="Black"
                  getPressed="getPressed"
                  onAction="rangeController" />
                <toggleButton id="highlight-color-white"
                  image="HighlightWhiteImg"
                  label="White"
                  getPressed="getPressed"
                  onAction="rangeController" />
              </menu>
            </menu>
          </splitButton>
        </group>
        <group id="pictures-controller"
          getLabel="getLabel"
          imageMso="PictureColorMenu">
          <toggleButton id="arrange"
            getSize="getSize"
            getImage="getImage"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            getPressed="getPressed"
            onAction="pictureController" />
          <toggleButton id="auto-arrange"
            getSize="getSize"
            getImage="getImage"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            getPressed="getPressed"
            onAction="pictureController" />
          <button id="snipping"
            getSize="getSize"
            imageMso="Camera"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="pictureController" />
          <comboBox
            id="offset"
            sizeString="AAAA"
            imageMso="SizeToControlHeightAndWidth"
            getShowImage="getShowImage"
            getLabel="getLabel"
            getEnabled="getEnabled"
            getVisible="getVisible"
            getItemCount="getItemCount"
            getItemID="getItemID"
            getItemLabel="getItemLabel"
            getText="getText"
            onChange="offsetSelect" />
          <checkBox 
            id="rate-lock"
            getLabel="getLabel"
            getEnabled="getEnabled"
            getVisible="getVisible"
            getPressed="getPressed"
            onAction="pictureController" />
          <button  idMso="PictureInsertFromFile"
            label="Insert Picture"
            size="normal"/>
        </group>
        <group id="option"
          getLabel="getLabel"
          imageMso="EditConnectedSystem">
          <splitButton id="settings-split"
            getSize="getSize">
            <button id="settings"
              imageMso="CurrentViewSettings"
              getEnabled="getEnabled"
              getShowImage="getShowImage"
              getKeytip="getKeytip"
              getLabel="getLabel"
              getShowLabel="getShowLabel"
              getScreentip="getScreentip"
              getSupertip="getSupertip"
              onAction="accessSettings" />
            <menu id="settings-menu"
              itemSize="normal">
              <menu id="wifi"
                label="Wifi Export"
                imageMso="AudioFadeInTime">
                <button id="wifi-export-txt"
                  label="Export to txt"
                  imageMso="GetPowerQueryDataFromCsv"
                  onAction="internetController" />
                <button id="wifi-export-csv"
                  label="Export to csv"
                  imageMso="PivotTableBlankRowsRemove"
                  onAction="internetController" />
                <button id="wifi-export-json"
                  label="Export to json"
                  imageMso="DatabaseCopyDatabaseFile"
                  onAction="internetController" />
              </menu>
            </menu>
          </splitButton>
          <button id="refresh-ribbon"
            getSize="getSize"
            imageMso="AccessRefreshAllLists"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="addinController" />
          <button id="remove-addin"
            getSize="getSize"
            imageMso="AccessRecycleBin"
            getEnabled="getEnabled"
            getShowImage="getShowImage"
            getKeytip="getKeytip"
            getLabel="getLabel"
            getShowLabel="getShowLabel"
            getScreentip="getScreentip"
            getSupertip="getSupertip"
            getVisible="getVisible"
            onAction="addinController" />
        </group>
        <group id="infomation"
          getLabel="getLabel"
          image="Author">
          <separator id="info-separator-lelf" />
          <labelControl id="tool-name"
            getLabel="getLabel" />
          <labelControl id="version"
            getLabel="getLabel" />
          <separator id="info-separator-right" />
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>